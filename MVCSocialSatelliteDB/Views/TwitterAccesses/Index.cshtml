@model IEnumerable<MVCSocialSatelliteDB.Models.TwitterAccess>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccessToken)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccessTokenSecret)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ConsumerKey)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccessToken)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccessTokenSecret)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ConsumerKey)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ProfileID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ProfileID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ProfileID })
        </td>
    </tr>
}

</table>
